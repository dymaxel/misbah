<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="sales_commission_level_from_inherit_webshop" model="ir.ui.view">
            <field name="name">Sale Commission Level Form inherit webshop</field>
            <field name="model">sale.commission.level</field>
            <field name="inherit_id" ref="sales_commission_external_user.sales_commission_level_from"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='name']" position="after">
                    <field name="web_shop_agent_level"/>
                    <field name="web_shop_sales_person_level"/>
                    <field name="percentage"/>
                </xpath>
            </field>
        </record>

    </data>
</odoo>
